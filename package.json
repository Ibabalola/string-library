{
 "name": "string-library",
 "version": "1.0.0",
 "description": "A String reversal library",
 "main": "index.js",
 "scripts": {
  "test": "ava",
  "build": "rollup -c"
 },
 "ava": {
  "require": [
   "@babel/register",
   "esm"
  ],
  "babel": {
   "testOptions": {
    "presets": [
     "@ava/stage-4",
     "@babel/preset-env",
     [
      "@ava/transform-test-files",
      {
       "powerAssert": true
      }
     ]
    ]
   }
  }
 },
 "author": "Isaac Babalola",
 "license": "MIT",
 "private": false,
 "devDependencies": {
  "@ava/babel-preset-stage-4": "^4.0.0",
  "@ava/babel-preset-transform-test-files": "^6.0.0",
  "@babel/core": "^7.6.2",
  "@babel/preset-env": "^7.6.2",
  "@babel/register": "^7.6.2",
  "ava": "2.4.0",
  "esm": "^3.2.25",
  "rollup": "^1.21.4",
  "rollup-plugin-babel": "^4.3.3",
  "rollup-watch": "^4.3.1"
 },
 "dependencies": {}
}
